{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "gamertag": {
      "type": "string"
    },
    "team": {
      "type": "string",
      "default": ""
    },
    "region": {
      "type": "string",
      "default": ""
    },
    "profile_picture": {
      "type": "string"
    },
    "seed": {
      "type": "integer",
      "minimum": 1
    },
    "description": {
      "type": "string",
      "default": ""
    }
  },
  "required": ["gamertag"],
  "additionalProperties": false
}
