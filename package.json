{
  "name": "player-profile-viewer",
  "version": "1.0.0",
  "description": "View a player's profile as a browser source. Starter bundle for rhythm game tournaments.",
  "homepage": "",
  "author": {
    "name": "Starrodkirby86",
    "email": "majestic.mudkip@gmail.com",
    "url": "https://ongaku.io"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "ddr",
    "esports",
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "^2.0.0",
    "dashboardPanels": [
      {
        "name": "playerProfile",
        "title": "Player Profile Editor",
        "width": 4,
        "file": "panel.html",
        "workspace": "Kirby's Quickstart",
        "headerColor": "#5f0f40"
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "width": 1920,
        "height": 1080,
        "singleInstance": false
      }
    ],
    "assetCategories": [
      {
        "name": "profilePictures",
        "title": "Profile Pictures",
        "allowedTypes": [
          "jpg",
          "jpeg",
          "png"
        ]
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "clean": "rimraf dashboard graphics",
    "build": "pnpm clean && cross-env NODE_ENV=production webpack",
    "start": "pnpm watch",
    "watch": "cross-env NODE_ENV=development webpack --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.9.6",
    "@nodecg/react-hooks": "^1.0.1",
    "classnames": "^2.5.1",
    "framer-motion": "^11.0.9",
    "graphql": "^16.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@nodecg/types": "^2.1.12",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^18.19.23",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "autoprefixer": "^10.4.18",
    "babel-loader": "^8.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "daisyui": "^4.7.3",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "glob": "^10.3.10",
    "html-webpack-plugin": "^5.6.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.8.1",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.35",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^9.5.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.4",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^36.0.0",
    "tailwindcss": "^3.4.1",
    "terser-webpack-plugin": "^5.3.10",
    "trash-cli": "^5.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.2",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.10.0"
  },
  "license": "Apache-2.0"
}
